<UserControl x:Class="LeetCodePlugin.ToolWindow1Control"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:vsshell="clr-namespace:Microsoft.VisualStudio.Shell;assembly=Microsoft.VisualStudio.Shell.15.0"
             Background="{DynamicResource {x:Static vsshell:VsBrushes.WindowKey}}"
             Foreground="{DynamicResource {x:Static vsshell:VsBrushes.WindowTextKey}}"
             mc:Ignorable="d"
             d:DesignHeight="629.5" d:DesignWidth="300"
             Name="MyToolWindow">
    <Grid>
        <Label Content="Description:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="#FFFFFEFE"/>
        <StackPanel Margin="10,40,-10,0">
            <TextBlock TextWrapping="Wrap" Foreground="#FFD6D5D5"><Run Text="TextBlock"/><LineBreak/><Run Language="zh-cn" Text="12341234"/><LineBreak/><Run/><LineBreak/><Run/></TextBlock>
            <Button Height="21" Click="SetCookieBtnClick" Margin="0,10,0,0" Content="SetCookie">
            </Button>
            <Button Content="Edit" Height="21" Click="EditBtnClick" Margin="0,10,0,0"/>
            <Button Content="Previous" Height="21" Click="PreviousBtnClick" Margin="0,10,0,0"/>
            <Button Content="Next" Click="NextBtnClick" Margin="0,10,0,0"/>
        </StackPanel>
    </Grid>
</UserControl>
